<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="UserIconView">
        <attr name="default_image" />
        <attr name="image_radius" />
    </declare-styleable>

    <declare-styleable name="SynthesizedImageView">
        <attr name="synthesized_image_bg" />
        <attr name="synthesized_default_image" />
        <attr name="synthesized_image_size" />
        <attr name="synthesized_image_gap" />
    </declare-styleable>

    <declare-styleable name="LineControllerView">
        <attr name="name" />
        <attr name="subject" />
        <attr name="isBottom" />
        <attr name="isTop" />
        <attr name="canNav" />
        <attr name="isSwitch" />
    </declare-styleable>

    <declare-styleable name="IndexBar">
        <attr name="indexBarTextSize" />
        <attr name="indexBarPressBackground" />
    </declare-styleable>

    <declare-styleable name="core_round_rect_image_style">
        <attr name="round_radius" />
    </declare-styleable>

    <declare-styleable name="TitleBarLayout">
        <attr name="title_bar_middle_title" />
        <attr name="title_bar_can_return" />
    </declare-styleable>

    <declare-styleable name="RoundFrameLayout">
        <attr name="corner_radius" />
        <attr name="left_top_corner_radius" />
        <attr name="right_top_corner_radius" />
        <attr name="right_bottom_corner_radius" />
        <attr name="left_bottom_corner_radius" />
    </declare-styleable>

    <declare-styleable name="RoundCornerImageView" parent="RoundFrameLayout">
        <attr name="corner_radius" />
        <attr name="left_top_corner_radius" />
        <attr name="right_top_corner_radius" />
        <attr name="right_bottom_corner_radius" />
        <attr name="left_bottom_corner_radius" />
    </declare-styleable>

    <declare-styleable name="UnreadCountTextView">
        <attr name="paint_color" />
    </declare-styleable>

    <declare-styleable name="SwitchCustomWidth">
        <attr name="custom_width" />
    </declare-styleable>

    <declare-styleable name="SwipeLayout">
        <attr name="drag_edge" />
        <attr name="leftEdgeSwipeOffset" />
        <attr name="rightEdgeSwipeOffset" />
        <attr name="topEdgeSwipeOffset" />
        <attr name="bottomEdgeSwipeOffset" />
        <attr name="show_mode" />
        <attr name="clickToClose" />
    </declare-styleable>


    <declare-styleable name="max_width_style">
        <attr name="maxWidth" format="dimension" />
    </declare-styleable>

</resources>