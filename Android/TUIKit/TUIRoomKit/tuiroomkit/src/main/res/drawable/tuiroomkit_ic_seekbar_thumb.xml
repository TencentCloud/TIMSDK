<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="oval">
            <solid android:color="#ffebf4ff" />
            <size
                android:width="18dp"
                android:height="18dp" />
        </shape>
    </item>

    <item
        android:top="1dp"
        android:bottom="1dp"
        android:right="1dp"
        android:left="1dp">
        <shape
            android:shape="oval">
            <solid android:color="@color/tuiroomkit_color_white" />
            <size
                android:width="18dp"
                android:height="18dp" />
        </shape>
    </item>

    <item
        android:top="5dp"
        android:bottom="5dp"
        android:right="5dp"
        android:left="5dp">
        <shape
            android:shape="oval">
            <solid android:color="@color/tuiroomkit_color_blue" />
            <size
                android:width="13dp"
                android:height="13dp" />
        </shape>
    </item>

</layer-list>