<view class="audio-message {{isMine?'my-audio':''}}">
  <view class="audio "   bindtap="handlePlayAudioMessage" data-value="{{item}}" style="width: {{120 + message.renderDom[0].second * 7}}rpx">
    <image class="image {{isMine?'my-image':''}}" src="../../../../static/images/sendingaduio.png"/> {{renderDom[0].second}}s
  </view>
</view>

