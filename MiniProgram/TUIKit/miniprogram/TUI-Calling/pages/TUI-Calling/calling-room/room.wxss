.btn-operate {
	display: flex;
	justify-content: space-between;
	position: absolute;
	bottom: 5vh;
	width: 100%;
}
.btn-operate-item{
	display: flex;
	flex-direction: column;
	align-items: center;
}
.btn-operate-item text {
	padding: 8px 0;
	font-size: 18px;
	color: #FFFFFF;
	letter-spacing: 0;
	font-weight: 400;
}

.call-operate {
	width: 15vw;
	height: 15vw;
	border-radius: 15vw;
	padding: 5px;
	margin: 0 15vw;
}

.tips {
	font-size: 20px;
	color: #FFFFFF;
	letter-spacing: 0;
	text-align: right;
	text-shadow: 0 1px 2px rgba(0,0,0,0.40);
	font-weight: 600;
}
.tips-subtitle {
	font-family: PingFangSC-Regular;
	font-size: 14px;
	color: #FFFFFF;
	letter-spacing: 0;
	text-align: right;
	text-shadow: 0 1px 2px rgba(0,0,0,0.30);
	font-weight: 400;
}

.invite-call {
	/* background-image: url(https://mc.qcloudimg.com/static/img/7da57e0050d308e2e1b1e31afbc42929/bg.png); */
	background: #ffffff;
	position: absolute;
	top: 0;
	z-index: 100;
	width: 100vw;
	height: 100vh;
}
.invite-call .local-video {
	width: 100vw;
	height: 100vh;
}
.invite-call .invite-calling {
	position: absolute;
	top: 0;
	z-index: 101;
	width: 100vw;
	height: 100vh;
}
.invite-calling-header {
	margin-top:107px;
	display: flex;
	justify-content: space-between;
	padding: 0 16px;
	
}
.invite-calling-header-left image {
	width: 32px;
	height: 32px;
}
.invite-calling-header-right {
	display: flex;
	align-items: center;
}
.invite-calling-header-message {
	display: flex;
	flex-direction: column;
	padding: 0 16px;
}
.invite-calling-header-right image {
	width: 100px;
	height: 100px;
	border-radius: 12px;
}
.invite-calling .btn-operate{
	display: flex;
	justify-content: center;
	position: absolute;
	bottom: 5vh;
	width: 100%;
}

